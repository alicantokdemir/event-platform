{
  "name": "event-service",
  "version": "0.1.0",
  "private": true,
  "main": "dist/apps/event-service/src/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/event-service/src/main.js",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest -c jest.config.cjs",
    "test:unit": "jest -c jest.config.cjs",
    "test:integration": "jest -c jest.integration.config.cjs",
    "test:e2e": "jest -c jest.e2e.config.cjs",
    "migration:generate": "typeorm-ts-node-commonjs -d src/db/typeorm.datasource.ts migration:generate",
    "migration:run": "typeorm-ts-node-commonjs -d src/db/typeorm.datasource.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d src/db/typeorm.datasource.ts migration:revert",
    "migration:show": "typeorm-ts-node-commonjs -d src/db/typeorm.datasource.ts migration:show"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.654.0",
    "@nestjs/common": "^10.4.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/platform-express": "^10.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/swagger": "^7.4.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "pg": "^8.12.0",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.0",
    "@nestjs/testing": "^10.4.0",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}